{
  "manifest_version": 2,

  "name": "EasyGrid",
  "description": "This extension generates new html using web content.",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "My First Time ;)"
  },
  "content_scripts": {
    "matches": ["<all_urls>"],
    "js": ["js/content.js"]
  },
  "permissions": [
    "tabs",
    "https://ajax.googleapis.com/"
  ]
}
